azul = #2C6BFC;
azul_fuerte = #074687;
gris = #484848;
morado = #9190F7;
celeste = #44D2FB;
header
	background-color celeste
	color azul_fuerte
	width 100%
	text-align center
	margin 0 0 1em 0
	padding 0.5em
	overflow hidden
	img
		float left
		margin 0 10px 0 50px
	h1
		margin 0
		padding 0
		
body
	background-color black
	text-align center
	.contenedor
		width 100%
		height 100%
		padding 1em
		.datos
			background-color azul
			border-radius 0.5em
			box-shadow 5px 5px 15px black
			width 316px
			padding 0.2em
			margin 0 20px 0 0
			float left
			#reloj
				background-color black
				color white
				text-align center
				font-size 40pt
				margin 0
				padding 0
				width 100%
		iframe
			border none
			border-radius 0.5em
			box-shadow 5px 5px 15px black
			display block 
			margin 0
			padding 0
			position relative
			left 0px
			width 950px
			height 90%

		.datos , iframe
			//display inline-block
			vertical-align top